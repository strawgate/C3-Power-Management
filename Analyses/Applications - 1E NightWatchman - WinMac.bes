		<Property Name="NightWatchman - Shutdown Message - Windows" ID="24" EvaluationPeriod="P1D">if (Windows of Operating System) then (value "ShutdownMsg" of key "1E\NightWatchman" of key "HKEY_LOCAL_MACHINE\Software" of native registry) else nothing</Property>
		<Property Name="NightWatchman - Monday Scheduled Action - Win\Mac" ID="26" EvaluationPeriod="P1D">if (Windows of Operating System) then (  (item 0 of it * hour + item 1 of it * minute) of (values "MondayPowerdownHour" of it as string as integer, values "MondayPowerdownMinutes" of it as string as integer), values "MondayLogoffAction" of it as string as uppercase, values "MondayPowerdownAction" of it as string as uppercase) of keys "HKEY_LOCAL_MACHINE\Software\1E\NightWatchman\ScheduledActions" of native registry   else (  (string "Time" of it, string "LogOff" of it as uppercase, string "Action" of it as uppercase) of dictionaries whose (string "Day" of it = "Monday") of values of array "ScheduledActions" of dictionary of file "/Library/Preferences/com.1E.NightWatchman.plist" )</Property>

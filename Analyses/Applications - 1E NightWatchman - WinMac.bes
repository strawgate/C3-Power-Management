	<Analysis>
		<Title>Applications - 1E NightWatchman - Win\Mac</Title>
		<Description><![CDATA[<P>Provides information on the current 1E NightWatchman configuration.</P>
<P>For general information or to report issues with C3 Power Management content please visit GitHub here: <A href="https://github.com/strawgate/C3-Power-Management">https://github.com/strawgate/C3-Power-Management</A></P>]]></Description>
		<Relevance>Windows of Operating System or Mac of operating system</Relevance>
		<Relevance>if (Windows of Operating System) then (exists key "HKEY_LOCAL_MACHINE\Software\1E\NightWatchman" of native registry) else (exists file "/Library/Preferences/com.1E.NightWatchman.plist")</Relevance>
		<Domain>BESC</Domain>
		<Property Name="NightWatchman - Version - Win\Mac" ID="1" EvaluationPeriod="P1D">if (windows of operating system) then (value "ProductVersion" of key "HKEY_LOCAL_MACHINE\Software\1E\NightWatchman" of native registry)   else (version of folder "/Library/Application Support/1E/NightWatchman/Agent.app")</Property>
		<Property Name="NightWatchman - System Tray Visible - Win\Mac" ID="2" EvaluationPeriod="P1D">if (windows of operating system) then (exists values "SystemTray" whose (it as string as uppercase = "ON") of key "HKEY_LOCAL_MACHINE\Software\1E\NightWatchman" of native registry)  else (boolean "ShowSystemTray" of dictionary "SysTraySettings" of dictionary of file "/Library/Preferences/com.1E.NightWatchman.plist")</Property>
		<Property Name="NightWatchman - Policy Number - Windows" ID="3" EvaluationPeriod="P1D">if (Windows of Operating System) then (value "PolicyNumber" of keys "HKEY_LOCAL_MACHINE\Software\1E\WakeUpAgt" of native registries)  else nothing</Property>
		<Property Name="NightWatchman - Disk Idle Timeout on AC - Win\Mac" ID="4" EvaluationPeriod="P1D">(if (it = "0") then ("Disabled") else (it)) of (  if (windows of operating system) then (value "DiskAC" of key "HKEY_LOCAL_MACHINE\Software\1E\NightWatchman\PowerSettings" of native registry as string)  else (integer "DiskAC" of dictionary "PowerSettings" of dictionary of file "/Library/Preferences/com.1E.NightWatchman.plist" as string)  )</Property>
		<Property Name="NightWatchman - Disk Idle Timeout on Battery - Win\Mac" ID="5" EvaluationPeriod="P1D">(if (it = "0") then ("Disabled") else (it)) of (  if (windows of operating system) then (value "DiskDC" of key "HKEY_LOCAL_MACHINE\Software\1E\NightWatchman\PowerSettings" of native registry as string)  else (integer "DiskDC" of dictionary "PowerSettings" of dictionary of file "/Library/Preferences/com.1E.NightWatchman.plist" as string)  )</Property>
